#include <iostream>
#include <ctime>
using namespace std;


int sum(int a, int b)
{
  return a + b;
}

int sum(int a, int b, int c)
{
  return a + b + c;
}

double sum(double a, double c)
{
  return a + c;
}

int main()
{
  cout << sum(3, 2) << endl;
  cout << sum(3,2,2) << endl;
  cout << sum(2.4, 4.2) << endl;
}
